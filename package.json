{
  "name": "nstcg-website",
  "version": "1.0.0",
  "description": "North Swanage Traffic Consultation Group community action website",
  "type": "module",
  "scripts": {
    "both": "concurrently -n \"vite,api\" -c \"blue,green\" \"npm:dev:vite\" \"npm:dev:vercel\"",
    "both:preview": "npm run build && npm run preview",
    "dev:vite": "vite",
    "dev:vercel": "vercel dev",
    "start": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "node tests/run-e2e-tests.js",
    "test:e2e:headed": "npm run test:e2e -- --headed",
    "test:e2e:debug": "npm run test:e2e -- --debug",
    "test:e2e:ui": "npm run test:e2e -- --ui",
    "test:e2e:chrome": "npm run test:e2e -- --project=chromium",
    "test:e2e:firefox": "npm run test:e2e -- --project=firefox",
    "test:e2e:mobile": "npm run test:e2e -- --project=\"Mobile Chrome\"",
    "test:e2e:ci": "CI=true npm run test:e2e",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "compile-email": "node scripts/compile-email.js",
    "generate:local": "node scripts/utilities/signup-runner.js",
    "generate:dry-run": "node scripts/utilities/signup-runner.js --dry-run --verbose",
    "generate:prod": "node scripts/utilities/signup-runner.js --api-url https://nstcg.org/api/submit-form"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.4",
    "@types/yargs": "^17.0.33",
    "@vercel/node": "^5.3.2",
    "@vitejs/plugin-legacy": "^5.4.2",
    "concurrently": "^9.2.0",
    "dotenv": "^17.0.1",
    "faker": "^6.6.6",
    "jest": "^30.0.3",
    "node-fetch": "^3.3.2",
    "open": "^10.0.3",
    "openai": "^5.8.2",
    "pg": "^8.16.3",
    "terser": "^5.36.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.10",
    "yargs": "^18.0.0"
  },
  "engines": {
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/nstcg-website.git"
  },
  "keywords": [
    "community",
    "traffic-safety",
    "swanage",
    "dorset",
    "consultation"
  ],
  "author": "NSTCG",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^3.1.3",
    "@sendgrid/mail": "^8.1.5",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1",
    "micromodal": "^0.6.1",
    "resend": "^4.6.0",
    "uuid": "^11.1.0"
  }
}